	<animation>
    <type>select</type>
    <object-name>LandingLightFlash</object-name>
    <object-name>LandingLightRemb</object-name>
    <object-name>LandingLightCone</object-name>
         
    <condition>
      <equals>
        <property>controls/switches/landing-light</property>
        <value> 0 </value>
      </equals>
    </condition>
  </animation> 
  
  
